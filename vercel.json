{
  "crons": [
    {
      "path": "/api/cron/import-markets?publish=true",
      "schedule": "0 0 * * *"
    },
    {
      "path": "/api/cron/settle-markets?secret=${CRON_SECRET}",
      "schedule": "59 23 * * *"
    },
    {
      "path": "/api/cron/publish-markets?secret=${CRON_SECRET}",
      "schedule": "*/10 * * * *"
    }
  ]
}
